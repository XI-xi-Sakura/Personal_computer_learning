[DEFAULT]

[logger]
level=DEBUG

[routing:static]
bind_address=0.0.0.0
routing_strategy=round-robin
protocol=classic
client_ssl_mode=PREFERRED
server_ssl_mode=PREFERRED
server_ssl_verify=DISABLED

[http_server]
ssl=0

[mysql_rest_service]
mysql_read_write_route=static
wait_for_metadata_schema_access=320

